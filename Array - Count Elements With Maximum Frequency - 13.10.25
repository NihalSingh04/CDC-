class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map<int,int> mpp;
        for(int i=0;i<nums.size();i++)
        {
            mpp[nums[i]]++;
        } 

        int ans=0,maxi=0;
        for(auto it=mpp.begin();it!=mpp.end();it++)
        {
            if(it->second>maxi)
            {
                maxi=it->second;
                ans=0;
            }
            if(it->second==maxi) ans+=it->second;
        }
        return ans;
    }
};
