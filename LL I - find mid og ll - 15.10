#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node* findMiddle(Node* head) {
    if (!head) return nullptr;
    Node* slow = head;
    Node* fast = head;
    while (fast && fast->next) {
        slow = slow->next;
        fast = fast->next->next;
    }
    return slow;
}

int main() {
    Node* head = new Node{10, new Node{20, new Node{30, new Node{40, nullptr}}}};
    Node* mid = findMiddle(head);
    if (mid)
        cout << "Middle Node: " << mid->data << endl;
    return 0;
}
