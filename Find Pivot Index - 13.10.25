class Solution {
public:
    int pivotIndex(vector<int>& nums) {
        int sum=0;
        for(int i=0;i<nums.size();i++)
        {
            sum+=nums[i];
        }
        if(sum-nums[0]==0) return 0;
        sum=sum-nums[0];
        int l=0;
        for(int i=1;i<nums.size();i++)
        {
            sum-=nums[i];
            l+=nums[i-1];
            if(sum==l) return i;
        }
        return -1;

    }
};
