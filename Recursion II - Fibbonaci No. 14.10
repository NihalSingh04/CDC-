#include <iostream>
using namespace std;

// Recursive Fibonacci function
int fib(int n)
{
  if (n <= 1)
    return n;
  return fib(n - 1) + fib(n - 2);
}

int main()
{
  int n;
  cout << "Enter n: ";
  cin >> n;
  cout << "Fibonacci(" << n << ") = " << fib(n) << endl;
  return 0;
}

/*
Explanation:
Classic recursive definition:
fib(n) = fib(n-1) + fib(n-2)

Time Complexity: O(2^n)
Space Complexity: O(n)
*/
